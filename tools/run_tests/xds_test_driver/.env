# Test server
SERVER_NAME='psm-grpc-server'
SERVER_TEST_PORT=8080
SERVER_MAINTENANCE_PORT=8081
SERVER_REPLICA_COUNT=1
#SERVER_DEBUG_REUSE_SERVICE=1
SERVER_KSA="${SERVER_NAME}"
SERVER_IAM_MEMBER="serviceAccount:${WORKLOAD_POOL}[${NAMESPACE}/${SERVER_KSA}]"

# Test server xDS settings
SERVER_XDS_HOST='sergii-psm-test-xds-host'
# todo(sergiitk): debug different port number
SERVER_XDS_PORT=8000
# Used in cleanup scripts
NEG_NAME="${CLUSTER_NAME_PRIMARY}-grpc-test-server-java"

# Backend service (Traffic Director)
