# GCP
PROJECT_ID='grpc-testing'
NETWORK_NAME='default-vpc'

# Default SAs
SA_GCE="830293263384-compute@developer.gserviceaccount.com"
SA_GKE="service-830293263384@container-engine-robot.iam.gserviceaccount.com"

# GKE Cluster
REGION_PRIMARY="us-central1"
REGION_PRIMARY_ZONE_PRIMARY="$REGION_PRIMARY-a"
REGION_PRIMARY_ZONE_SECONDARY="$REGION_PRIMARY-c"
#CLUSTER_NAME="interop-test-psm-sec1"
CLUSTER_NAME="sergiitk-interop-dev"
CLUSTER_NAME_PRIMARY="${CLUSTER_NAME}-${REGION_PRIMARY}"
WORKLOAD_POOL="${PROJECT_ID}.svc.id.goog"

# K8S
KUBE_CONTEXT_NAME="${PROJECT_ID}-${CLUSTER_NAME}"
NAMESPACE='sergii-psm-test'

# Test client
CLIENT_NAME='psm-grpc-client'
CLIENT_DEBUG_USE_PORT_FORWARDING=1
CLIENT_KSA="${CLIENT_NAME}"
